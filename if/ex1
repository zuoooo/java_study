package iff;

import java.util.Scanner;

public class ifex3 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int score = scanner.nextInt();
		char grade = 0 ; 
		
		if(score>=90) {
			grade = 'A';
		}
		else if (score>=80) {
			grade = 'B';
		}
		else if (score>=70) {
			grade = 'C';
		}
		else if (score>=60) {
			grade = 'D';
		}
		else if (score>=50) {
			grade = 'F';
		}
		System.out.println("점수 : "+ score);
		System.out.println("학점 :" + grade);
	}
}
